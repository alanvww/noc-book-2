{
  "title": "Nature of Code",
  "addPlugins": [
    "magicbook-codesplit",
    "magicbook-katex"
  ],
  "destination": "build",
  "files": [
    "content/*.html"
  ],
  "images": {
    "files": [
      "content/**/*.jpg",
      "content/**/*.png"
    ]
  },
  "builds": [
    {
      "format": "pdf",
      "layout": "magicbook/layouts/pdf.html",
      "stylesheets": {
        "files": [
          "magicbook/stylesheets/pdf.scss"
        ]
      },
      "fonts": {
        "files": "magicbook/fonts/**/*.*",
        "destination": "assets/fonts/"
      }
    }
  ]
}